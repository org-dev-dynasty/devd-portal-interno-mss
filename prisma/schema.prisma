datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String     @id @default(uuid())
  name      String
  email     String     @unique
  role      String
  password  String
  telefone  String     @unique
  cpf       String     @unique
  status    String
}